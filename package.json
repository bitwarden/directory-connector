    "@angular/platform-browser-dynamic": "21.1.1",
    "@angular/router": "21.1.1",
    "@microsoft/microsoft-graph-client": "3.0.7",
    "big-integer": "1.6.52",
    "bootstrap": "5.3.7",
    "browser-hrtime": "1.1.8",
